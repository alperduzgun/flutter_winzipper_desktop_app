# Production Environment Configuration
APP_NAME=WinZipper
FLAVOR=production

# API Configuration
API_BASE_URL=https://api.winzipper.com
API_TIMEOUT=30000

# Cloud Upload
CLOUD_UPLOAD_SERVICE=litterbox
CLOUD_UPLOAD_URL=https://litterbox.catbox.moe/resources/internals/api.php
CLOUD_RETENTION_HOURS=72

# Features
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_LOGGING=false
LOG_LEVEL=error

# Archive Settings
MAX_ARCHIVE_SIZE_MB=1000
SUPPORTED_FORMATS=zip,rar,7z,tar,gz
